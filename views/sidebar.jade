h1
    .avatar
        img(src="../assets/images/avatar.jpg")
    span Luke Stebner
ul
    // 0 index is the slug for the url
    // 1 index is the label
    - items = [["home", "Home/About"], ["beats", "Beats"], ["work", "Work"]]
    for item, i in items
        li
            a(href="#" + item[0]) 
                span.icon &gt;
                = item[1]
